include config.mk

PROJ_PROCS_SUB_HEADERS		= $(shell find ./src/proc/%%NXS_TPL_P_RPATH%% -mindepth 2 -maxdepth 2 -type f -name "*.h")

objs/%%NXS_TPL_P_OBJ%%.o: ./src/proc/%%NXS_TPL_P_RPATH%%%%NXS_TPL_P_FILENAME%%.c \
				./src/proc/%%NXS_TPL_P_RPATH%%%%NXS_TPL_P_FILENAME%%.h \
				./src/proc/%%NXS_TPL_P_RPATH%%%%NXS_TPL_P_FILENAME%%-subproc.h \
				$(PROJ_CORE_HEADERS) \
				$(PROJ_META_HEADERS) \
				$(PROJ_UNITS_HEADERS) \
				$(PROJ_COLLECTOINS_HEADERS) \
				$(PROJ_PROC_SUB_HEADERS) \
				$(NXS_CORE_HEADERS)

				$(CC) -c $(CFLAGS) $(CFLAGS32) $(NXS_CORE_INCLUDES) $(PROJ_INCLUDES) $(NXS_INCLUDES) $(USE_NXS_MODULES) -o objs/%%NXS_TPL_P_OBJ%%.o ./src/proc/%%NXS_TPL_P_RPATH%%%%NXS_TPL_P_FILENAME%%.c
